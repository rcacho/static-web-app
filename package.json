{
  "name": "calendar-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build  && rm -rf ./.next/cache",
    "start": "next start",
    "lint": "next lint",
    "check-ts": "tsc --noEmit --skipLibCheck --noUnusedLocals --noUnusedParameters",
    "check-console": "yarn get-diff | egrep '^.[[[:digit:]]+m\\+' | grep 'console.log\\|console.time\\|console.timeEnd' && exit 1 || exit 0",
    "check-prettier": "yarn prettier --config .prettierrc --check 'src/**/*.ts*'",
    "check-all": "yarn check-ts && yarn check-console && yarn check-prettier"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.34.0",
    "@azure/msal-node": "^1.16.0",
    "@azure/msal-react": "^1.5.4",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/utils": "^4.11.3",
    "@microsoft/microsoft-graph-client": "^3.0.5",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.9",
    "@next/font": "13.1.6",
    "@react-pdf/renderer": "^3.1.8",
    "@types/node": "18.11.19",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "file-saver": "^2.0.5",
    "html-to-image": "^1.11.11",
    "html2canvas": "^1.4.1",
    "html2canvas-render-offscreen": "^0.0.3",
    "ics": "^3.1.0",
    "jsonwebtoken": "^9.0.0",
    "jspdf": "^2.5.1",
    "jwks-rsa": "^3.0.1",
    "mssql": "^9.1.1",
    "mui": "^0.0.1",
    "net": "^1.0.2",
    "next": "13.1.6",
    "next-api-middleware": "^2.0.1",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-pdf": "^6.2.2",
    "react-promise-tracker": "^2.1.1",
    "react-window": "^1.8.8",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/file-saver": "^2.0.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mssql": "^8.1.2",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "prettier": "^2.8.4"
  }
}
