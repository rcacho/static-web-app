{
  "name": "calendar-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build  && rm -rf ./.next/cache",
    "start": "next start",
    "lint": "next lint",
    "check-ts": "tsc --noEmit --skipLibCheck --noUnusedLocals --noUnusedParameters",
    "check-console": "yarn get-diff | egrep '^.[[[:digit:]]+m\\+' | grep 'console.log\\|console.time\\|console.timeEnd' && exit 1 || exit 0",
    "check-prettier": "yarn prettier --config .prettierrc --check 'src/**/*.ts*'",
    "check-all": "yarn check-ts && yarn check-console && yarn check-prettier",
    "test": "set TS_NODE_COMPILER_OPTIONS='./tsconfig.commonjs.json' mocha -r ts-node/register test/**/APIManager.spec.ts*"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.34.0",
    "@azure/msal-node": "^1.16.0",
    "@azure/msal-react": "^1.5.4",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/utils": "^4.11.3",
    "@microsoft/microsoft-graph-client": "^3.0.5",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.9",
    "@next/font": "13.1.6",
    "@types/chai-as-promised": "^7.1.5",
    "@types/fs-extra": "^11.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.11",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "axios": "^1.3.4",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "esm": "^3.2.25",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "mocha": "^10.2.0",
    "mssql": "^9.1.1",
    "mui": "^0.0.1",
    "net": "^1.0.2",
    "next": "13.2.4",
    "next-api-middleware": "^2.0.1",
    "node": "^19.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-window": "^1.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "module": "CommonJS"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.1",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "prettier": "^2.8.4"
  }
}
